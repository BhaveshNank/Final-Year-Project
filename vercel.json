{
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/chat",
      "dest": "/app.py"
    },
    {
      "src": "/health",
      "dest": "/app.py"
    },
    {
      "src": "/test",
      "dest": "/app.py"
    },
    {
      "src": "/products/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/images/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/debug-(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/reset-session",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}