{
  "version": 2,
  "builds": [
    {
      "src": "simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "/simple.py"
    },
    {
      "src": "/api/simple",
      "dest": "/simple.py"
    },
    {
      "src": "/(.*)",
      "dest": "/simple.py"
    }
  ]
}