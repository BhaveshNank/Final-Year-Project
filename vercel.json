{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.py"
    },
    {
      "src": "/chat",
      "dest": "/index.py"
    },
    {
      "src": "/health",
      "dest": "/index.py"
    },
    {
      "src": "/test",
      "dest": "/index.py"
    },
    {
      "src": "/images/(.*)",
      "dest": "/index.py"
    },
    {
      "src": "/debug-(.*)",
      "dest": "/index.py"
    },
    {
      "src": "/reset-session",
      "dest": "/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ]
}